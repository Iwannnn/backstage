<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.iwannnn.backstage.mapper.SysUserMapper">

	<select id="selectByUsername" resultType="cn.iwannnn.backstage.domain.entity.SysUser">
	    select *
	    from sys_user
		where username = #{username}
	</select>

	<select id="slectAll" resultType="cn.iwannnn.backstage.domain.entity.SysUser">
		select *
		from sys_user
	</select>
</mapper>