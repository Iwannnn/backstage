<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.iwannnn.backstage.mapper.UserMapper">

	<select id="selectAccount" resultType="cn.iwannnn.backstage.dto.UserDto">
	    select *
	    from sys_user
		where account = #{account}
	</select>
</mapper>